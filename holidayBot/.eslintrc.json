{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "semi": 0,
    "quotes": 0,
    "linebreak-style": 0,
    "lines-between-class-members": 0,
    "comma-dangle": 0,
    "consistent-return": 0,
    "object-curly-newline": 0,
    "no-console": 1,
    "import/extensions": 0,
    "no-underscore-dangle": 0,
    "no-undef": 1,
    "no-unused-vars": 1,
    "import/no-extraneous-dependencies": 0,
    "no-shadow": 0,
    "class-methods-use-this": 0,
    "operator-linebreak": 0,
    "function-paren-newline": 0,
    "implicit-arrow-linebreak": 0
  }
}
